{
    "manifest_version": 2,
    "name": "Update Checker",
    "description": "__MSG_description__",
    "default_locale": "en",
    "version": "0.24.5",
    "background": {
        "page": "/background.html"
    },
    "browser_action": {
        "default_icon": "/img/logo.png"
    },
    "icons": {
        "128": "/img/logo.png"
    },
    "options_page": "/options.html",
    "permissions": ["tabs", "alarms", "background", "storage", "notifications", "*://*/*"]
}
