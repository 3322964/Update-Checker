{
    "manifest_version": 2,
    "name": "Update Checker",
    "version": "0.28.1",
    "default_locale": "en",
    "description": "__MSG_description__",
    "icons": {
        "128": "/img/logo.png"
    },
    "browser_action": {
        "default_icon": "/img/logo.png"
    },
    "background": {
        "scripts": ["/js/background.js"]
    },
    "permissions": ["tabs", "storage", "unlimitedStorage", "*://*/*"]
}
